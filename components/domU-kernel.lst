$mkdir /boot/grub
/boot/kernel
$write /boot/grub/menu.lst 'default=0\ntimeout=1\n\ntitle Linux\nroot (hd0)\nkernel (hd0)/boot/kernel root=/dev/xvda1'
$write /boot/grub/grub.cfg "set default=0\nset timeout=1\n\nmenuentry 'Linux' {\n\techo 'Loading kernel ...'\n\tlinux /boot/kernel root=/dev/xvda1\n}"
