$exec --overlay "grub-mkimage -p /boot/grub -o /boot.img -O i386-pc-eltorito loopback xfs fat part_gpt part_msdos normal linux echo all_video test multiboot multiboot2 search iso9660 gzio lvm chain configfile cpuid minicmd gfxterm font terminal ata biosdisk squash4 videoinfo videotest png gfxterm_background"
$exec --overlay "grub-mkimage -p /boot/grub -o /bootx64.efi -O x86_64-efi xfs fat part_gpt part_msdos normal linux echo all_video test multiboot multiboot2 search iso9660 gzio lvm chain configfile cpuid minicmd gfxterm font terminal loopback squash4 png gfxterm_background"
$exec --overlay "dd if=/dev/zero of=/efiboot.img bs=4k count=360 && mkfs.vfat -F 12 -M 0xf8 /efiboot.img && mount -o loop -t vfat /efiboot.img /mnt && mkdir -p /mnt/efi/boot && cp -av /bootx64.efi /mnt/efi/boot/ && umount /mnt"
